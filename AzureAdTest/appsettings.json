{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "Key": "<Your JWT key here>",
    "Issuer": "http://localhost:5001",
    "ExpireInMinutes": "30"
  },
  "Saml2": {
    "EntityId": "<Azure AD App Entity Id>",
    "ReturnUrl": "https://localhost:5001/auth/callback",
    "IdentityProvider": {
      "MicrosoftEntraIdentifier": "<Azure AD Identifier URL>",
      "MetadataLocation": "<Azure AD Metadata URL or the absolute path to metadata xml file in your machine>"
    }
  }
}
